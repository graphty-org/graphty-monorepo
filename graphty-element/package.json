{
    "name": "@graphty/graphty-element",
    "version": "1.5.4",
    "description": "A Web Component library for 3D/2D graph visualization built with Lit and Babylon.js",
    "type": "module",
    "main": "./dist/graphty.umd.cjs",
    "module": "./dist/graphty.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/graphty.js",
            "require": "./dist/graphty.umd.cjs"
        }
    },
    "sideEffects": [
        "./dist/graphty.js",
        "./dist/graphty.umd.cjs",
        "./src/layout/index.ts",
        "./src/data/index.ts",
        "./src/algorithms/index.ts"
    ],
    "bin": {
        "graphty-log-server": "./bin/graphty-log-server.js"
    },
    "files": [
        "dist/",
        "bin/",
        "README.md",
        "LICENSE"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "keywords": [
        "graph",
        "visualization",
        "3d",
        "web-components",
        "lit",
        "babylonjs",
        "typescript"
    ],
    "author": "Adam Powers <apowers@ato.ms>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/graphty-org/graphty-monorepo.git",
        "directory": "graphty-element"
    },
    "homepage": "https://github.com/graphty-org/graphty-monorepo/tree/master/graphty-element#readme",
    "bugs": {
        "url": "https://github.com/graphty-org/graphty-monorepo/issues"
    },
    "scripts": {
        "test": "npm run test:shards:parallel && npm run test:visual",
        "test:default": "vitest --project=default",
        "test:browser": "vitest --project=browser",
        "test:storybook": "vitest --project=storybook",
        "test:visual": "npx chromatic",
        "test:shard:default": "npm run test:shard:default:run",
        "test:shard:default:run": "vitest run --project=default",
        "test:shard:browser": "npm run test:shard:browser:run",
        "test:shard:browser:run": "vitest run --project=browser --project=interactions",
        "test:shard:storybook": "npm run test:shard:storybook:run",
        "test:shard:storybook:run": "vitest run --project=storybook",
        "test:shard:visual": "npx chromatic",
        "test:shards": "npm run test:shard:default && npm run test:shard:browser && npm run test:shard:storybook && npm run test:shard:visual",
        "test:shards:parallel": "npm run test:shard:default:run & npm run test:shard:browser:run & npm run test:shard:storybook:run & wait",
        "test:visual:debug": "npx chromatic --debug",
        "test:mesh": "vitest run --config test/mesh-testing/vitest-mesh.config.ts",
        "test:mesh:watch": "vitest --config test/mesh-testing/vitest-mesh.config.ts",
        "test:mesh:coverage": "vitest run --config test/mesh-testing/vitest-mesh.config.ts --coverage",
        "test:interactions": "vitest run --project=interactions",
        "test:interactions:unit": "vitest run --project=interactions test/interactions/unit/",
        "test:interactions:browser": "vitest run --project=interactions test/interactions/integration/",
        "test:interactions:xr": "vitest run --project=interactions test/interactions/xr/",
        "test:interactions:edge-cases": "vitest run --project=interactions test/interactions/edge-cases/",
        "test:llm-regression": "vitest run --project llm-regression",
        "test:llm-regression:watch": "vitest --project llm-regression",
        "coverage": "npm run coverage:shards && npm run coverage:merge",
        "coverage:merge": "./scripts/coverage-merge.sh",
        "coverage:shards": "npm run coverage:shards:clean && npm run coverage:shards:run",
        "coverage:shards:clean": "rm -rf .coverage-parts",
        "coverage:shards:run": "npm run coverage:shard:default & npm run coverage:shard:browser & npm run coverage:shard:storybook & wait",
        "coverage:shard:default": "COVERAGE_DIR=.coverage-parts/default vitest run --project=default --coverage",
        "coverage:shard:browser": "npm run coverage:shard:browser:1 & npm run coverage:shard:browser:2 & npm run coverage:shard:browser:3 & npm run coverage:shard:browser:4 & npm run coverage:shard:browser:5 & wait",
        "coverage:shard:browser:1": "COVERAGE_DIR=.coverage-parts/browser-1 vitest run --project=browser --project=interactions --shard=1/5 --coverage",
        "coverage:shard:browser:2": "COVERAGE_DIR=.coverage-parts/browser-2 vitest run --project=browser --project=interactions --shard=2/5 --coverage",
        "coverage:shard:browser:3": "COVERAGE_DIR=.coverage-parts/browser-3 vitest run --project=browser --project=interactions --shard=3/5 --coverage",
        "coverage:shard:browser:4": "COVERAGE_DIR=.coverage-parts/browser-4 vitest run --project=browser --project=interactions --shard=4/5 --coverage",
        "coverage:shard:browser:5": "COVERAGE_DIR=.coverage-parts/browser-5 vitest run --project=browser --project=interactions --shard=5/5 --coverage",
        "coverage:shard:storybook": "npm run coverage:shard:storybook:1 & npm run coverage:shard:storybook:2 & npm run coverage:shard:storybook:3 & npm run coverage:shard:storybook:4 & wait",
        "coverage:shard:storybook:1": "COVERAGE_DIR=.coverage-parts/storybook-1 vitest run --project=storybook --shard=1/4 --coverage",
        "coverage:shard:storybook:2": "COVERAGE_DIR=.coverage-parts/storybook-2 vitest run --project=storybook --shard=2/4 --coverage",
        "coverage:shard:storybook:3": "COVERAGE_DIR=.coverage-parts/storybook-3 vitest run --project=storybook --shard=3/4 --coverage",
        "coverage:shard:storybook:4": "COVERAGE_DIR=.coverage-parts/storybook-4 vitest run --project=storybook --shard=4/4 --coverage",
        "coverage:fast": "vitest run --project=default --coverage",
        "coverage:preview": "npx serve coverage -p 9053",
        "lint": "eslint && tsc --noEmit",
        "lint:fix": "eslint --fix",
        "lint:pkg": "knip",
        "lint:all": "npm run lint && npm run lint:pkg",
        "dev": "vite --force --mode development",
        "dev:xr": "node examples/xr/xr-demo-server.js",
        "build": "vite build && tsc --project tsconfig.build.json",
        "preview": "vite preview",
        "build:storybook": "npm run build-storybook",
        "storybook": "rm -rf node_modules/.vite && npm run build-storybook && . ./.env 2>/dev/null; storybook dev -p ${PORT:-6006} --host ${HOST:-localhost} ${HTTPS_CERT_PATH:+--https --ssl-cert $HTTPS_CERT_PATH --ssl-key $HTTPS_KEY_PATH} --no-open",
        "chromatic": "npx chromatic --exit-zero-on-changes",
        "build-storybook": "storybook build",
        "ready:commit": "npm run build && npm run lint && npm test",
        "merge:upstream": "git pull origin master --no-rebase",
        "docs:api": "typedoc && node scripts/sanitize-api-docs.js",
        "docs:api:watch": "typedoc --watch",
        "docs:dev": "vitepress dev docs",
        "docs:build": "npm run docs:api && vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "devDependencies": {
        "@babylonjs/core": "^8.16.2",
        "@chromatic-com/storybook": "^4.0.0",
        "@eslint/js": "^9.29.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@storybook/addon-console": "^3.0.0",
        "@storybook/addon-docs": "^9.0.11",
        "@storybook/addon-vitest": "^9.0.11",
        "@storybook/test": "^8.6.14",
        "@storybook/web-components-vite": "^9.0.11",
        "@types/glob": "^8.1.0",
        "@types/hammerjs": "^2.0.46",
        "@types/jmespath": "^0.15.2",
        "@types/lodash": "^4.17.19",
        "@types/node": "^20.19.2",
        "@types/toposort": "^2.0.7",
        "@vitest/browser": "^3.1.4",
        "@vitest/coverage-v8": "^3.1.4",
        "chromatic": "^13.1.2",
        "eruda": "^3.4.3",
        "eslint": "^9.29.0",
        "eslint-plugin-jsdoc": "^50.6.17",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "fast-check": "^4.2.0",
        "iwer": "^2.1.1",
        "knip": "^5.61.3",
        "lit": "^3.3.1",
        "playwright": "^1.49.0",
        "pngjs": "^7.0.0",
        "semantic-release": "^25.0.2",
        "storybook": "^9.0.11",
        "typedoc": "^0.28.15",
        "typedoc-plugin-markdown": "^4.9.0",
        "typedoc-vitepress-theme": "^1.1.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.34.1",
        "vite": ">=5.4.19",
        "vite-plugin-cem": "^0.8.2",
        "vite-plugin-eslint": "^1.8.1",
        "vitepress": "^1.6.3",
        "vitest": "^3.1.4"
    },
    "peerDependencies": {
        "@babylonjs/core": "^8.0.0",
        "@mlc-ai/web-llm": ">=0.2.0",
        "lit": "^3.0.0"
    },
    "peerDependenciesMeta": {
        "@mlc-ai/web-llm": {
            "optional": true
        }
    },
    "dependencies": {
        "@ai-sdk/anthropic": "^2.0.50",
        "@ai-sdk/google": "^2.0.44",
        "@ai-sdk/openai": "^2.0.74",
        "@graphty/algorithms": "workspace:*",
        "@graphty/layout": "workspace:*",
        "@jsonhero/schema-infer": "^0.1.5",
        "@logtape/logtape": "^1.2.2",
        "@types/papaparse": "^5.5.0",
        "ai": "^5.0.104",
        "colorjs.io": "^0.5.2",
        "d3-force-3d": "^3.0.6",
        "encrypt-storage": "^2.14.7",
        "fast-xml-parser": "^5.3.1",
        "hammerjs": "^2.0.8",
        "jmespath": "^0.16.0",
        "lodash": "^4.17.21",
        "ngraph.forcelayout": "^3.3.1",
        "ngraph.graph": "^20.0.1",
        "on-change": "^5.0.1",
        "p-queue": "^8.1.1",
        "papaparse": "^5.5.3",
        "pupt": "^1.2.1",
        "selfsigned": "^2.4.1",
        "toposort": "^2.0.2",
        "zod": "^3.25.28"
    },
    "overrides": {
        "storybook": "$storybook"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
