{
  "name": "@graphty/graphty-element",
  "version": "1.0.4",
  "description": "A Web Component library for 3D/2D graph visualization built with Lit and Babylon.js",
  "type": "module",
  "main": "./dist/graphty.umd.cjs",
  "module": "./dist/graphty.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/graphty.js",
      "require": "./dist/graphty.umd.cjs"
    }
  },
  "sideEffects": [
    "./dist/graphty.js",
    "./dist/graphty.umd.cjs",
    "./src/layout/index.ts",
    "./src/data/index.ts",
    "./src/algorithms/index.ts"
  ],
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "graph",
    "visualization",
    "3d",
    "web-components",
    "lit",
    "babylonjs",
    "typescript"
  ],
  "author": "Adam Powers <apowers@ato.ms>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphty-org/graphty-element.git"
  },
  "homepage": "https://github.com/graphty-org/graphty-element",
  "bugs": {
    "url": "https://github.com/graphty-org/graphty-element/issues"
  },
  "scripts": {
    "test:visual:ui": "node scripts/playwright-ui-https.js",
    "test": "vitest --project=default",
    "test:browser": "vitest --project=browser",
    "test:storybook": "vitest --project=storybook",
    "test:all": "vitest run --project=storybook --project=default --project=browser && npm run test:visual",
    "test:visual": "npm run test:visual:generate && playwright test --project=visual --workers=1",
    "test:visual:fast": "npm run test:visual:generate && playwright test --project=visual-fast --workers=1",
    "test:visual:update": "npm run test:visual:generate && playwright test --project=visual --update-snapshots --workers=1",
    "test:visual:fast:update": "npm run test:visual:generate && playwright test --project=visual-fast --update-snapshots --workers=1",
    "test:visual:generate": "tsx test/playwright/playwright-generator.ts",
    "test:visual:ui": "npm run test:visual:generate && playwright test --ui",
    "test:visual:report": "npx playwright show-report --host=${PLAYWRIGHT_REPORT_HOST:-localhost} --port=${PLAYWRIGHT_REPORT_PORT:-9323}",
    "test:visual:benchmark": "node scripts/benchmark-fast-tests.js",
    "test:visual:ui:https": "npx concurrently --kill-others --names proxy,playwright \"npx local-ssl-proxy --source 9003 --target 9002 --cert ~/ssl/atoms.crt --key ~/ssl/atoms.key\" \"npx playwright test --ui --ui-host=0.0.0.0 --ui-port=9002\"",
    "coverage": "vitest --project=storybook --project=default --project=browser --coverage",
    "lint": "eslint && tsc --noEmit",
    "lint:fix": "eslint --fix",
    "lint:pkg": "knip",
    "lint:all": "npm run lint && npm run lint:pkg",
    "dev": "vite --force --mode development",
    "build": "tsc --project tsconfig.build.json && vite build",
    "build:chromatic": "npx tsx test/helpers/scripts/chromatic-generator-simple.ts",
    "preview": "vite preview",
    "build:storybook": "npm run build-storybook",
    "storybook": "storybook dev -p 9025 --no-open",
    "storybook:chromatic": "npm run build:chromatic && CHROMATIC=true storybook dev -p 9025 --no-open",
    "chromatic": "npm run build:chromatic && CHROMATIC=true npx chromatic --exit-zero-on-changes",
    "prepare": "husky",
    "postinstall": "playwright install",
    "build-storybook": "storybook build",
    "ready:commit": "npm run build && npm run lint && npm run test:all",
    "merge:upstream": "git pull origin master --no-rebase"
  },
  "devDependencies": {
    "@babylonjs/core": "^8.16.2",
    "@chromatic-com/storybook": "^4.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.54.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-docs": "^9.0.11",
    "@storybook/addon-vitest": "^9.0.11",
    "@storybook/test": "^8.6.14",
    "@storybook/web-components-vite": "^9.0.11",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@types/glob": "^8.1.0",
    "@types/hammerjs": "^2.0.46",
    "@types/jmespath": "^0.15.2",
    "@types/lodash": "^4.17.19",
    "@types/node": "^20.19.2",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "chromatic": "^13.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eruda": "^3.4.3",
    "eslint": "^9.29.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "knip": "^5.61.3",
    "lit": "^3.3.1",
    "playwright": "^1.53.0",
    "semantic-release": "^24.2.7",
    "storybook": "^9.0.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": ">=5.4.19",
    "vite-plugin-cem": "^0.8.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "@babylonjs/core": "^8.0.0",
    "lit": "^3.0.0"
  },
  "dependencies": {
    "@graphty/layout": "^1.2.8",
    "colorjs.io": "^0.5.2",
    "d3-force-3d": "^3.0.6",
    "hammerjs": "^2.0.8",
    "jmespath": "^0.16.0",
    "lodash": "^4.17.21",
    "ngraph.forcelayout": "^3.3.1",
    "ngraph.graph": "^20.0.1",
    "on-change": "^5.0.1",
    "zod": "^3.25.28"
  },
  "overrides": {
    "storybook": "$storybook"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
