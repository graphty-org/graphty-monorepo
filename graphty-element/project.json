{
    "name": "graphty-element",
    "$schema": "../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "graphty-element/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": ["{projectRoot}/dist"],
            "options": {
                "command": "npm run build",
                "cwd": "graphty-element"
            },
            "dependsOn": ["^build"]
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run test:shards:parallel",
                "cwd": "graphty-element"
            }
        },
        "test:ui": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vitest --ui",
                "cwd": "graphty-element"
            }
        },
        "test:coverage": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vitest run --coverage",
                "cwd": "graphty-element"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "eslint && tsc --noEmit",
                "cwd": "graphty-element"
            }
        },
        "semantic-release": {
            "executor": "nx:run-commands",
            "dependsOn": ["build"],
            "options": {
                "command": "semantic-release",
                "cwd": "graphty-element"
            }
        },
        "storybook": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run storybook",
                "cwd": "graphty-element"
            }
        },
        "build-storybook": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run build-storybook",
                "cwd": "graphty-element"
            }
        },
        "test:visual": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run test:visual",
                "cwd": "graphty-element"
            }
        },
        "chromatic": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run chromatic",
                "cwd": "graphty-element"
            }
        },
        "docs:build": {
            "executor": "nx:run-commands",
            "outputs": ["{projectRoot}/docs/.vitepress/dist"],
            "options": {
                "command": "npm run docs:build",
                "cwd": "graphty-element"
            },
            "dependsOn": ["build"]
        }
    }
}
