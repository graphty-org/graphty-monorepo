<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
         http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

  <!-- Key definitions -->
  <key id="d0" for="node" attr.name="label" attr.type="string"/>
  <key id="d1" for="node" attr.name="size" attr.type="double"/>
  <key id="d2" for="node" attr.name="color" attr.type="string"/>
  <key id="d3" for="edge" attr.name="weight" attr.type="double"/>
  <key id="d4" for="edge" attr.name="label" attr.type="string"/>

  <graph id="G" edgedefault="directed">
    <!-- Central hub node -->
    <node id="n0">
      <data key="d0">Hub</data>
      <data key="d1">30</data>
      <data key="d2">#ff6b6b</data>
    </node>

    <!-- Spoke nodes -->
    <node id="n1">
      <data key="d0">Alpha</data>
      <data key="d1">20</data>
      <data key="d2">#4ecdc4</data>
    </node>

    <node id="n2">
      <data key="d0">Beta</data>
      <data key="d1">20</data>
      <data key="d2">#45b7d1</data>
    </node>

    <node id="n3">
      <data key="d0">Gamma</data>
      <data key="d1">20</data>
      <data key="d2">#96ceb4</data>
    </node>

    <node id="n4">
      <data key="d0">Delta</data>
      <data key="d1">20</data>
      <data key="d2">#ffeaa7</data>
    </node>

    <node id="n5">
      <data key="d0">Epsilon</data>
      <data key="d1">20</data>
      <data key="d2">#dfe6e9</data>
    </node>

    <!-- Edges from hub to spokes -->
    <edge source="n0" target="n1">
      <data key="d3">1.0</data>
      <data key="d4">primary</data>
    </edge>

    <edge source="n0" target="n2">
      <data key="d3">0.8</data>
      <data key="d4">strong</data>
    </edge>

    <edge source="n0" target="n3">
      <data key="d3">0.6</data>
      <data key="d4">medium</data>
    </edge>

    <edge source="n0" target="n4">
      <data key="d3">0.4</data>
      <data key="d4">weak</data>
    </edge>

    <edge source="n0" target="n5">
      <data key="d3">0.2</data>
      <data key="d4">minimal</data>
    </edge>

    <!-- Cross-connections between spokes -->
    <edge source="n1" target="n2">
      <data key="d3">0.5</data>
      <data key="d4">cross</data>
    </edge>

    <edge source="n2" target="n3">
      <data key="d3">0.5</data>
      <data key="d4">cross</data>
    </edge>

    <edge source="n3" target="n4">
      <data key="d3">0.5</data>
      <data key="d4">cross</data>
    </edge>

    <edge source="n4" target="n5">
      <data key="d3">0.5</data>
      <data key="d4">cross</data>
    </edge>

    <edge source="n5" target="n1">
      <data key="d3">0.5</data>
      <data key="d4">cross</data>
    </edge>
  </graph>
</graphml>
